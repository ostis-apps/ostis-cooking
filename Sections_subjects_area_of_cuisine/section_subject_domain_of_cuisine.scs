concept_eastern_cuisine
    <-sc_node_not_relation;
    =>nrel_main_idtf:[восточная кухня](*<-lang_ru;;*);
    =>nrel_main_idtf:[eastern cuisine](*<-lang_en;;*);;

concept_european_cuisine
    <-sc_node_not_relation;
    =>nrel_main_idtf:[европейская кухня](*<-lang_ru;;*);
    =>nrel_main_idtf:[european cuisine](*<-lang_en;;*);;
   
section_subject_domain_of_african_cuisine
    =>nrel_main_idtf:[Раздел. Предметная область африканской кухни] (* <-lang_ru;;*);
