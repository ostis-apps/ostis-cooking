        <- definition;;
        => nrel_idtf: [Определение (Терияки) ] (* <-lang_ru;; *);;
        <= nrel_sc_text_translation: ... (*
            -> rrel_example: [ Терияки - это традиционный японский способ обжарки с использованием распространённого в японской кухне сладкого соуса. ] (* <-lang_ru;; *);;
        *);;*);
        => nrel_recipe: ... (*
            -> rrel_ingredients: ... (*
                -> concept_chicken_fillet;;
                -> concept_soy_sauce;;
                -> concept_brown_sugar;;
                -> concept_ginger;;
                -> concept_onion;;
                -> concept_garlic;;
                -> concept_sake;;
                -> concept_vegetable_oil;;
                -> concept_sesame;;
                -> concept_green_onions;;
            *);;
            -> rrel_instruction:...(*
