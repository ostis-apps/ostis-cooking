            *);;
        *);;
    *);

    => nrel_cooking_time: 5_min (*
        => nrel_main_idtf: [5 минут](* <- lang_ru;; *);;
        <- concept_min;;
    *);

    => nrel_portion_number: 1_portion (*
        =>nrel_main_idtf: [1 порция](* <-lang_ru;; *);;
