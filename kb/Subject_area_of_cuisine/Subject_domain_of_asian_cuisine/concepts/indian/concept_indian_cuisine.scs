        concept_lentil_soup;
        concept_biryani;
        concept_dal_makhani;
        concept_vindaloo;
        concept_paneer_masala;
        concept_murgh_makhani;
        concept_chicken_tikka_masala;;

concept_indian_cuisine
    <-sc_node_not_relation;
    =>nrel_main_idtf:[индийская кухня](*<-lang_ru;;*);
    =>nrel_main_idtf:[indian cuisine](*<-lang_en;;*);
    =>nrel_specifics: ... (*
        => nrel_main_idtf: [ В целом индийская кухня славится разнообразием вкусовых предпочтений, и ее можно описать как кисло-сладко-пряно-острую. Основные продукты в ней – это изделия из муки, рис, рыба, овощи, лепешки и, конечно, молочные продукты. ](* <-lang_ru;; *);;
    *);
    <-concept_cuisine;;
