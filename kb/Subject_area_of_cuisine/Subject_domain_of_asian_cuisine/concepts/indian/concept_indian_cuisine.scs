    <-atomic_section;
    ->rrel_key_sc_element: subject_domain_of_indian_cuisine;;  


subject_domain_of_indian_cuisine
    =>nrel_main_idtf:[Предметная область индийской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain of indian cuisine] (* <-lang_en;;*);
	<-subject_domain; 
