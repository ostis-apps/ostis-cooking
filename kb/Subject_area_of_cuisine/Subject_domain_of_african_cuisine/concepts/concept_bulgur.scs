            -> concept_leek;;
            -> concept_fresh_mint;;
            -> concept_parsley;;
            -> concept_olive_oil;;
            -> concept_tomato_paste;;
            -> concept_lemon;;
            -> concept_paprika;;
            -> concept_salt;;
            -> concept_black_pepper;;
        *);;
        <-rrel_instruction: ...(*
            => nrel_idtf: [ Рецепт ( Булгур-салат) ] (* <-lang_ru;; *);;
            <= nrel_sc_text_translation: ... (*
                -> rrel_example: "file://../figures/html/bulgur.txt"
                (*
                => nrel_format: format_txt;;
                *);;
            *);;
        *);;     
    *);;

