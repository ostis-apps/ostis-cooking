        <-rrel_ingredients: ...(*
            -> concept_paprika;;
            -> concept_garlic;;
            -> concept_olive_oil;;
            -> concept_coriander;;
            -> concept_carrot;;
            -> concept_water;; 
            -> concept_salt;; 
            -> concept_lemon_juice;; 
