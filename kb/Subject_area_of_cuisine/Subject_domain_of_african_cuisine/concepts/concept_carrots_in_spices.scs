        <-rrel_instruction: ...(*
            => nrel_idtf: [ Рецепт ( Морковь в специях) ] (* <-lang_ru;; *);;
            <= nrel_sc_text_translation: ... (*
                -> rrel_example: "file://../figures/html/carrots_in_spices.txt"
                (*
                => nrel_format: format_txt;;
                *);;
            *);;
        *);;     
    *);;

