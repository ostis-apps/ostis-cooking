subject_domain_of_norwegian_cuisine
    =>nrel_main_idtf:[Предметная область норвежской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain of norwegian cuisine] (* <-lang_en;;*);
	<-subject_domain; 
    ->rrel_maximum_studied_object_class:
        concept_norwegian_cuisine;
    ->rrel_not_maximum_studied_object_class:
	concept_creamy_salmon_soup;
