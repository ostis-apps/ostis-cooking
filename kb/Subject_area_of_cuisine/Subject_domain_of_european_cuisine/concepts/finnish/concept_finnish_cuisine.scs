section_subject_domain_of_finnish_cuisine
    =>nrel_main_idtf:[Раздел. Предметная область финской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Section. Subject domain of finnish cuisine] (* <-lang_en;;*);
    =>nrel_idtf:[Раздел базы знаний OSTIS, описывающий финскую кухню] (* <-lang_ru;;*);
    =>nrel_idtf:[Section of the OSTIS knowledge base, that describes finnish cuisine] (* <-lang_en;;*);
    <-atomic_section;
    ->rrel_key_sc_element: subject_domain_of_finnish_cuisine;;


subject_domain_of_finnish_cuisine
    =>nrel_main_idtf:[Предметная область финской кухни] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain of finnish cuisine] (* <-lang_en;;*);
	<-subject_domain; 
    ->rrel_maximum_studied_object_class:
        concept_finnish_cuisine;;

subject_domain_of_finnish_cuisine
