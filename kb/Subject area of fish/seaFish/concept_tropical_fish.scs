      (*<-lang_ru;;=>nrel_format:format_html;;*);;*);;
  *);;

 concept_tropical_fish -> rrel_example: [ Тропическая рыба обитает в таких течениях, как Гольфстрим, Северное атлантическое, Карибское, Южнопассатное, Ломоносова, Гвинейское, Бразильское и др.](* <- lang_ru;; *);;

